<context>

	<Resource 
		name="jdbc/DriveDS" 
		auth="Container" 
		type="javax.sql.DataSource"
        maxActive="100" 
        maxIdle="30" 
        maxWait="10000"
        username="access" 
        password="access" 
        driverClassName="com.mysql.jdbc.Driver"
        defaultAutoCommit="false"
    	defaultTransactionIsolation="READ_COMMITTED"
    	url="jdbc:mysql://localhost:3306/access"/>

